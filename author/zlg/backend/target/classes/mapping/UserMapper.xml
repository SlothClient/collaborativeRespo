<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jxutcm.equipmentMaintance.Dao.UserDao">
    <select id="doLogin" parameterType="com.jxutcm.equipmentMaintance.Bean.User" resultType="com.jxutcm.equipmentMaintance.Bean.User">
        select * from 用户表 where uname = #{uname} and upwd = #{upwd}
    </select>

</mapper>
