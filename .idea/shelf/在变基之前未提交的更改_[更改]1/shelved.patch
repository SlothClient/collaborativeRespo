+<script setup>
+import {ref, watch} from "vue";
+import {ElNotification} from "element-plus";
+
+const props = defineProps({
+  checkInfoEditVisible:{
+    type:Boolean,
+    default:false,
+  },
+  currentRow: {
+    type: Object,
+    default: () => null,
+  },
+  equipmentInfoList: {
+    type: Array,
+    default: () => [],
+  },
+});
+const dialogVisible = ref(props.checkInfoEditVisible);
+const emit = defineEmits(['closeDialog', 'editCheckInfo']);
+
+const planDate = ref([]);
+
+watch(
+    () => props.checkInfoEditVisible,
+    (newValue) => {
+      dialogVisible.value = newValue;
+      if (props.currentRow) {
+        editPlan.value = {...props.currentRow};
+        planDate.value = [editPlan.value.startTime, editPlan.value.endTime];
