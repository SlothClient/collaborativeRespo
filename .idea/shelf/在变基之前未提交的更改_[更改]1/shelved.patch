+  editPlan.value.endTime = planDate.value[1];
+
+  // 日志输出以调试
+  console.log(editPlan.value);
+
+  // 简化的空值校验逻辑
+  const fieldsToCheck = ['checkName', 'equipId', 'checkDesc', 'checkSite','checkContent', 'startTime'];
+  const hasEmptyField = fieldsToCheck.some(field => !editPlan.value[field] || editPlan.value[field].trim() === '');
+
+  if (hasEmptyField) {
+    ElNotification({
+      message: '不能存在空值，请重试',
+      type: 'error',
+    });
+    return;
+  }
+
+  // 如果没有空值，继续进行后续逻辑
+  emit("editCheckInfo", editPlan.value);
+  closeDialog(); // 关闭对话框
+};
+
+  const editPlan = ref({
