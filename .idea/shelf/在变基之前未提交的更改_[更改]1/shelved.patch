+  endTime: null,
+});
+const clearForm = () => {
+  editPlan.value = {
+    equipId: '',
+    planName: '',
+    checkDesc: '',
+    checkSite:'',
+    checkContent:'',
+    startTime: null,
+    endTime: null,
+  };
+  planDate.value = [];
+};
+</script>
