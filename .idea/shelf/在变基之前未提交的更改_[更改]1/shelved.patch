+  dialogVisible.value = false;
+  emit('closeDialog');
+};
+
+const confirmEdit = () => {
+  // 检查 planDate 是否为数组并且不为空
+  if (!Array.isArray(planDate.value) || planDate.value.length < 2 || !planDate.value[0] || !planDate.value[1]) {
