+    top: 48px;
+    width: 100%;
+    border-bottom: 1px solid #ccc;
+}
+
+/* 工作记录输入框 */
+#record {
+    border: none;
+    width: 100%;
+    height: 100px;
+    resize: none;
+}
+
+#record:focus {
+    outline: none;
+}
+</style>
